{
    "reportID":"dashboard",
    "colCount": 4,
    "rowHeight": 30,
    "filterForm":{
      "colCount": 4,
      "rowCount": 1,
      "rowHeight": 30,
      "labelPos":"left",
      "controls": [
        {
          "field":"start_date",
          "label":"开始日期",
          "width":150,
          "controlType": "DatePicker",
          "defaultValue":"return moment().format('YYYY-MM-DD');",
          "row": 1,
          "col": 1,
          "colSpan": 1,
          "rowSpan": 1
        },
        {
          "field":"end_date",
          "label":"结束日期",
          "width":150,
          "controlType": "DatePicker",
          "defaultValue":"return moment().format('YYYY-MM-DD');",
          "row": 1,
          "col": 2,
          "colSpan": 1,
          "rowSpan": 1
        },
        {
          "label":"区域",
          "field":"area_type",
          "width":150,
          "fieldType":"many2one",
          "relatedModelID":"sl_area_type",
          "controlType": "SingleSelect",
          "defaultValue":"return {value:'001',list:[{id:'001',name:'低密区'}]};",
          "optionLabel":"name",
          "fields":[
              {"field":"id"},
              {"field":"name"}
          ],
          "row": 1,"col": 3,"colSpan": 1,"rowSpan": 1
        }
      ]
    },
    "controls": [
      {
          "id":"1",
          "controlType": "EChart",
          "minHeight":250,
          "option":{
              "title":{
                "text":"开锁次数占比",
                "show":true,
                "textStyle":{
                  "fontWeight":"normal",
                  "fontSize":12
                }
              },
              "color":["#91cc75","#fac858"],
              "legend":{
                "bottom":10
              },
              "dataset":{
                "source":[
                  {"lock_id": 0, "open_times": 0}
                ],
                "dimensions":["open_times","lock_id"]
              },
              "tooltip": {
                "trigger": "item"
              },
              "toolbox": {
                  "show": true,
                  "orient": "vertical",
                  "left": "right",
                  "top": "center",
                  "feature": {
                    "mark": { "show": true },
                    "dataView": { "show": true, "readOnly": false },
                    "saveAsImage": { "show": true }
                  }
                },
                "series": [{
                  "type":"pie",
                  "radius":80,
                  "label":{
                    "position":"inside",
                    "show":false
                  }
                }]
          },
          "sqlParameters":{
            "start_date":"return `'${filterData.start_date?filterData.start_date:moment().format('YYYY-MM-DD')}'`",
            "end_date":"return `'${filterData.end_date?filterData.end_date:moment().format('YYYY-MM-DD')}'`",
            "lock_area_filter":"return filterData.area_type?.value?`where area_type='${filterData.area_type.value}'`:'where 1=1'"
          },
          "query":"select * from (select lock_id,count(1) as open_times from smartlockv2.sl_lock_status_record where status='010' and create_time>=%{start_date} and create_time<=%{end_date} and lock_id in (select id from smartlockv2.sl_lock %{lock_area_filter}) group by lock_id) t order by open_times desc limit 0,10",
          "row": 1,
          "col": 1,
          "colSpan": 2,
          "rowSpan": 1
        },
        {
          "id":"2",
          "controlType": "EChart",
          "minHeight":250,
          "option":{
            "title":{
              "text":"人员申请次数",
              "show":true,
              "textStyle":{
                "fontWeight":"normal",
                "fontSize":12
              }
            },
            "color":["orange","green"],
            "legend":{
              "bottom":10
            },
            "dataset":{
              "source":[
                {"create_user": "", "app_count": 0}
              ],
              "dimensions":["create_user","app_count"]
            },
            "tooltip": {
                "trigger": "axis",
                "axisPointer": {
                    "type": "shadow"
                }
            },
            "toolbox": {
                "show": true,
                "orient": "vertical",
                "left": "right",
                "top": "center",
                "feature": {
                  "mark": { "show": true },
                  "dataView": { "show": true, "readOnly": false },
                  "magicType": { "show": true, "type": ["line", "bar", "stack"] },
                  "restore": { "show": true },
                  "saveAsImage": { "show": true }
                }
              },
              "grid":{
                "left":100
              },
              "xAxis": [
                {
                  "type": "category",
                  "axisTick": { "show": false }
                }
              ],
              "yAxis": [
                {
                  "type": "value",
                  "offset":0
                }
              ],
              "series": [{
                "type":"bar",
                "name":"",
                "barMaxWidth":40
                }
              ]
          },
          "query":"select * from (select create_user,count(1) as app_count from smartlockv2.sl_application group by create_user) t order by app_count desc limit 0,10",
          "row": 1,
          "col": 3,
          "colSpan": 2,
          "rowSpan": 1
        },
        {
            "id":"3",
            "controlType": "EChart",
            "minHeight":250,
            "option":{
              "title":{
                "text":"开锁次数",
                "show":true,
                "textStyle":{
                  "fontWeight":"normal",
                  "fontSize":12
                }
              },
              "color":["orange","green"],
              "legend":{
                "bottom":10
              },
              "dataset":{
                "source":[
                  {"open_date": "", "open_count": 0 }
                ],
                "dimensions":["open_date","open_count"]
              },
              "tooltip": {
                  "trigger": "axis",
                  "axisPointer": {
                      "type": "shadow"
                  }
              },
              "toolbox": {
                  "show": true,
                  "orient": "vertical",
                  "left": "right",
                  "top": "center",
                  "feature": {
                    "mark": { "show": true },
                    "dataView": { "show": true, "readOnly": false },
                    "magicType": { "show": true, "type": ["line", "bar", "stack"] },
                    "restore": { "show": true },
                    "saveAsImage": { "show": true }
                  }
                },
                "grid":{
                  "left":100
                },
                "xAxis": [
                  {
                    "type": "category",
                    "axisTick": { "show": false }
                  }
                ],
                "yAxis": [
                  {
                    "type": "value",
                    "offset":0
                  }
                ],
                "series": [{
                  "type":"line",
                  "name":"",
                  "barMaxWidth":40
                  }]
            },
            "sqlParameters":{
              "lock_area_filter":"return filterData.area_type?.value?`where area_type='${filterData.area_type.value}'`:'where 1=1'"
            },
            "query":"select * from (select DATE(create_time) as open_date,count(1) as open_count from smartlockv2.sl_lock_status_record where status='010' and lock_id in (select id from smartlockv2.sl_lock %{lock_area_filter}) group by DATE(create_time)) t order by open_date desc limit 0,30",
            "row": 2,
            "col": 1,
            "colSpan": 4,
            "rowSpan": 1
        }
    ]
}